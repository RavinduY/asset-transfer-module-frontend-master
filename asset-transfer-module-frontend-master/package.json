    "@reduxjs/toolkit": "^1.8.3",
    "antd": "^4.22.4",
    "axios": "^0.27.2",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.18.0",
    "react-pdf": "^5.7.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-to-print": "^2.14.11",
